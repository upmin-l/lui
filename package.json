{
  "name": "lui",
  "version": "0.0.1",
  "description": "A vue3.0 + based UI component library",
  "license": "MIT",
  "author": {
    "name": "upmin-l"
  },
  "main": "lib/index.js",
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist",
    "lib",
    "README.md"
  ],
  "scripts": {
    "lui:init": "yarn || npm i",
    "start": "npm run dev",
    "dev": "npm run build:entry && cross-env NODE_ENV=development vite",
    "build": "vite build",
    "clean:bag": "rimraf bag",
    "build:style": "rimraf lib/styles && gulp --gulpfile build/build-styles.js",
    "build:entry": "node build/build-entry-file.js"
  },
  "dependencies": {
    "highlight.js": "^11.1.0"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/component-compiler-utils": "3.1.2",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "del": "^5.1.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "json-templater": "^1.2.0",
    "marked": "^2.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5",
    "uppercamelcase": "^3.0.0",
    "vite": "^2.4.2",
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=5.5.1",
    "yarn": ">=1.3.2"
  }
}
