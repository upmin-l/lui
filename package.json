{
  "name": "lui",
  "version": "0.0.1",
  "description": "A vue3.0 + based UI component library",
  "license": "MIT",
  "author": {
    "name": "upmin-l"
  },
  "main": "lib/index.js",
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist",
    "lib",
    "README.md"
  ],
  "scripts": {
    "lui:init": "yarn || npm i",
    "start": "vite",
    "dev": "cross-env NODE_ENV=development node build/createRoutes.js && vite",
    "build": "vite build",
    "clean:bag": "rimraf lib",
    "new": "node build/new.js",
    "build:style": "rimraf lib/styles && gulp --gulpfile build/build-styles.js",
    "build:entry": "node build/build-entry-file.js",
    "dev:test": "cross-env NODE_ENV=development node build/createRoutes.js",
    "dev:bug": "cross-env NODE_ENV=bug node build/createRoutes.js && vite"
  },
  "dependencies": {
    "highlight.js": "^11.1.0",
    "lodash": "^4.17.21",
    "marked": "^2.1.3",
    "vue-router": "^4.0.10"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.0.5",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "del": "^5.1.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "inquirer": "^8.1.2",
    "json-templater": "^1.2.0",
    "rimraf": "^3.0.2",
    "sass": "~1.32.6",
    "typescript": "^4.3.5",
    "unplugin-vue-define-options": "^0.6.1",
    "uppercamelcase": "^3.0.0",
    "vite": "^2.6.13",
    "vue": "^3.2.21"
  },
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=5.5.1",
    "yarn": ">=1.3.2"
  }
}
